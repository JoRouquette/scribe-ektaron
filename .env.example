########################################
# .env – VPS : config du registry / image
# À copier depuis ce modèle :
#   cp .env.example .env
########################################

# URL de ton registry privé (hébergé sur ton VPS)
REGISTRY_URL=registry.scribe-ektaron.com

# Nom de l'image dans ce registry
IMAGE_NAME=scribe-ektaron

# Tag suivi par la prod (Watchtower suivra généralement "latest")
IMAGE_TAG=latest
