{
  "name": "obsidian-vps-publish",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/obsidian-vps-publish/src",
  "projectType": "application",
  "tags": ["layer:application", "type:plugin"],
  "targets": {
    "dev": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{workspaceRoot}/dist/apps/obsidian-vps-publish"],
      "options": {
        "platform": "node",
        "outputPath": "dist/apps/obsidian-vps-publish",
        "format": ["cjs"],
        "bundle": true,
        "main": "apps/obsidian-vps-publish/src/main.ts",
        "tsConfig": "apps/obsidian-vps-publish/tsconfig.json",
        "assets": [
          "apps/obsidian-vps-publish/manifest.json",
          "apps/obsidian-vps-publish/versions.json",
          "apps/obsidian-vps-publish/styles.css"
        ],
        "external": [
          "obsidian",
          "electron",
          "@codemirror/autocomplete",
          "@codemirror/collab",
          "@codemirror/commands",
          "@codemirror/language",
          "@codemirror/lint",
          "@codemirror/search",
          "@codemirror/state",
          "@codemirror/view",
          "@lezer/common",
          "@lezer/highlight",
          "@lezer/lr"
        ],
        "esbuildOptions": {
          "target": "es2018",
          "sourcemap": true,
          "outExtension": {
            ".js": ".js"
          }
        }
      }
    },
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{workspaceRoot}/dist/apps/obsidian-vps-publish"],
      "options": {
        "platform": "node",
        "outputPath": "dist/apps/obsidian-vps-publish",
        "format": ["cjs"],
        "bundle": true,
        "main": "apps/obsidian-vps-publish/src/main.ts",
        "tsConfig": "apps/obsidian-vps-publish/tsconfig.json",
        "assets": [
          "apps/obsidian-vps-publish/manifest.json",
          "apps/obsidian-vps-publish/versions.json",
          "apps/obsidian-vps-publish/styles.css"
        ],
        "external": [
          "obsidian",
          "electron",
          "@codemirror/autocomplete",
          "@codemirror/collab",
          "@codemirror/commands",
          "@codemirror/language",
          "@codemirror/lint",
          "@codemirror/search",
          "@codemirror/state",
          "@codemirror/view",
          "@lezer/common",
          "@lezer/highlight",
          "@lezer/lr"
        ],
        "esbuildOptions": {
          "target": "es2018",
          "sourcemap": false,
          "outExtension": {
            ".js": ".js"
          }
        }
      }
    },
    "package": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "outputs": ["{workspaceRoot}/dist/obsidian-vps-publish"],
      "options": {
        "command": "node apps/obsidian-vps-publish/scripts/package-plugin.mjs"
      }
    },
    "lint": {
      "executor": "nx:noop"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/obsidian-vps-publish"],
      "options": {
        "jestConfig": "apps/obsidian-vps-publish/jest.config.cjs",
        "runInBand": true
      }
    },
    "format": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prettier --check \"apps/obsidian-vps-publish/**/*.{ts,json,md,css,scss}\""
      }
    },
    "format:write": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prettier --write \"apps/obsidian-vps-publish/**/*.{ts,json,md,css,scss}\""
      }
    }
  }
}
